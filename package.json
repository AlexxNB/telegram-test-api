{
  "name": "telegram-test-api",
  "version": "1.0.3",
  "description": "Emulating telegram API",
  "keywords": [
    "telegram",
    "bot",
    "test"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node --use_strict ./bin/start",
    "test": "DEBUG=TelegramServer:* ./node_modules/mocha/bin/mocha --use_strict --exit",
    "lint": "./node_modules/eslint/bin/eslint.js src test",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/test.js -- --exit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jehy/telegram-test-api"
  },
  "author": "Jehy",
  "engines": {
    "node": ">=6.0"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "debug": "3.1.0",
    "express": "4.16.3",
    "http-shutdown": "1.2.0",
    "request": "^2.88.0",
    "request-promise": "4.2.2"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "istanbul": "0.4.5",
    "mocha": "5.0.4",
    "node-telegram-bot-api": "0.30.0"
  }
}
