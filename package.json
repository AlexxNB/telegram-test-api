{
  "name": "telegram-test-api",
  "version": "0.0.5",
  "description": "Emulating telegram API",
  "keywords": [
    "telegram",
    "bot",
    "test"
  ],
  "main": "script.js",
  "scripts": {
    "start": "node --use_strict ./bin/start",
    "test": "./node_modules/mocha/bin/mocha --use_strict --exit",
    "lint": "./node_modules/eslint/bin/eslint.js src test",
    "compile": "./node_modules/babel-cli/bin/babel.js --presets es2015 -d ./lib src",
    "prepublish": "npm run compile",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/test.js -- --exit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jehy/telegram-test-api"
  },
  "author": "Jehy",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "colors": "1.2.1",
    "express": "4.16.2",
    "request-promise": "4.2.2",
    "semver": "5.5.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "3.0.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "istanbul": "0.4.5",
    "mocha": "5.0.4",
    "node-telegram-bot-api": "0.30.0"
  }
}
